<Window x:Class="OESK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OESK"
        mc:Ignorable="d"
        Title="Test szybkości działania wybranych funkcji skrótu" MinWidth="650" MinHeight="300" Width="650" Height="300">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Test szybkości działania wybranych funkcji skrótu" VerticalAlignment="Top"/>
        <TextBlock Name="TxtBlockFullTime" HorizontalAlignment="Left" Margin="215,113,0,0" TextWrapping="Wrap" Text="Czas pełny" VerticalAlignment="Top"/>

        <TextBlock Name="MD5TxtBlockTime" HorizontalAlignment="Left" Margin="10,38,0,0" TextWrapping="Wrap" Text="Czas" VerticalAlignment="Top" Width="83"/>
        <TextBlock Name="MD5Title" HorizontalAlignment="Left" Margin="104,38,0,0" TextWrapping="Wrap" Text="MD5" VerticalAlignment="Top" Width="45"/>
        <TextBlock Name="MD5TxtBlockHash" Margin="177,38,10,0" TextWrapping="Wrap" Text="Tekst" VerticalAlignment="Top"/>

        <TextBlock Name="SHA1TxtBlockTime" HorizontalAlignment="Left" Margin="10,63,0,0" TextWrapping="Wrap" Text="Czas" VerticalAlignment="Top" Width="83"/>
        <TextBlock Name="SHA1Title" HorizontalAlignment="Left" Margin="104,63,0,0" TextWrapping="Wrap" Text="SHA1" VerticalAlignment="Top" Width="45"/>
        <TextBlock Name="SHA1TxtBlockHash" Margin="177,63,10,0" TextWrapping="Wrap" Text="Tekst" VerticalAlignment="Top"/>

        <TextBlock Name="SHA256TxtBlockTime" HorizontalAlignment="Left" Margin="10,88,0,0" TextWrapping="Wrap" Text="Czas" VerticalAlignment="Top" Width="83"/>
        <TextBlock Name="SHA256Title" HorizontalAlignment="Left" Margin="104,88,0,0" TextWrapping="Wrap" Text="SHA256" VerticalAlignment="Top" Width="45"/>
        <TextBlock Name="SHA256TxtBlockHash" Margin="177,88,10,0" TextWrapping="Wrap" Text="Tekst" VerticalAlignment="Top"/>

        <TextBlock HorizontalAlignment="Left" Margin="10,109,0,0" TextWrapping="Wrap" Text="Tekst do przetworzenia:" VerticalAlignment="Top"/>
        <TextBox Name="UserTxtBox" Margin="10,135,10,22" TextWrapping="Wrap" Text="Mój tekst"/>
        <Button Name="BtnStart" Content="Start" HorizontalAlignment="Left" Margin="139,109,0,0" VerticalAlignment="Top" Width="71" Click="BtnStart_Click"/>

        <Button Name="BtnStartStandardTest" Content="Uruchom pełny test" Margin="0,109,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="108" Click="BtnStartStandardTest_Click" />

    </Grid>
</Window>
